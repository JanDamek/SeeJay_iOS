#import "YoutubeViewController.h"

@implementation YoutubeViewController

- (void)viewDidLoad
{
    [super viewDidLoad];

    [youtube setDelegate:self];
	[youtube loadRequest:[NSURLRequest requestWithURL:[NSURL URLWithString:@"http://www.youtube.com/seejayradio"]]];
}

- (void)didReceiveMemoryWarning
{
    [super didReceiveMemoryWarning];
    // Dispose of any resources that can be recreated.
}

- (UIStatusBarStyle)preferredStatusBarStyle
{
    return UIStatusBarStyleLightContent;
}

-(IBAction)btnZpet:(id)sender{
    [youtube goBack];
}

-(IBAction)btnDopredu:(id)sender{
    [youtube goForward];
}

-(IBAction)btnStop:(id)sender{
    [youtube stopLoading];
}

-(IBAction)btnReload:(id)sender{
    [youtube reload];
}

-(void)webViewDidStartLoad:(UIWebView *)webView{
    [progres setHidden:NO];
}
-(void)webViewDidFinishLoad:(UIWebView *)webView{
    [progres setHidden:YES];
}

@end
